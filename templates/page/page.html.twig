{#
/**
 * @file
 * Template for the main page.
 */
#}
<div class="page">
  <main class="pt-5 pb-5">
    {% if page.header %}
      <div class="container">
        {{ page.header }}
      </div>
    {% endif %}

    {% if page.pre_content %}
      <div class="page__pre-content">
        <div class="container">
          {{ page.pre_content }}
        </div>
      </div>
    {% endif %}

    {% if page.content %}
      <div class="page__content">
        {{ page.content }}
      </div>
    {% endif %}
  </main>

  {% if page.footer %}
    <footer class="page__footer" id="asu-footer" role="contentinfo">
      {{ page.footer }}
    </footer>
  {% endif %}
</div>
