{#
/**
 * @file
 * Theme override to display a pager.
 *
 * Available variables:
 * - heading_id: Pagination heading ID.
 * - items: List of pager items.
 *   The list is keyed by the following elements:
 *   - first: Item for the first page; not present on the first page of results.
 *   - previous: Item for the previous page; not present on the first page
 *     of results.
 *   - next: Item for the next page; not present on the last page of results.
 *   - last: Item for the last page; not present on the last page of results.
 *   - pages: List of pages, keyed by page number.
 *   Sub-sub elements:
 *   items.first, items.previous, items.next, items.last, and each item inside
 *   items.pages contain the following elements:
 *   - href: URL with appropriate query parameters for the item.
 *   - attributes: A keyed list of HTML attributes for the item.
 *   - text: The visible text used for the item link, such as "‹ Previous"
 *     or "Next ›".
 * - current: The page number of the current page.
 * - ellipses: If there are more pages than the quantity allows, then an
 *   ellipsis before or after the listed pages may be present.
 *   - previous: Present if the currently visible list of pages does not start
 *     at the first page.
 *   - next: Present if the visible list of pages ends before the last page.
 *
 * @see template_preprocess_pager()
 */
#}
{% if items %}
  <nav role="navigation" aria-labelledby="{{ heading_id }}">
    {# Hidden title of pagination component (for accessibility) #}
    <h4 id="{{ heading_id }}" class="visually-hidden">{{ 'Pagination'|t }}</h4>

    {# Actual pagination markup #}
    <ul class="pagination">
      {# Render "Previous page" button (either enabled or disabled) #}
      {% if items.previous %}
        <li class="page-item">
      {% else %}
        <li class="page-item disabled">
      {% endif %}
          <a class="page-link page-link-icon" href="{{ items.previous.href }}" title="{{ 'Go to previous page'|t }}" rel="prev"{{ items.previous.attributes|without('href', 'title', 'rel') }}>
            <span class="visually-hidden">{{ 'Previous page'|t }}</span>
            <span aria-hidden="true">{{ 'Prev'|t }}</span>
          </a>
        </li>
      
      {% if current == 1 %}
        <li class="page-item active">
          <a class="page-link" href="{{ items.pages[current].href }}" title={{ 'Current' |t }}><span class="visually-hidden">{{ 'Current page' |t}}</span>{{ current }}</a>
        </li>
        {% if items.pages[current + 1] %}
          <li class="page-item">
            <a class="page-link" href="{{ items.pages[current + 1].href }}" title={{ 'Go to page @key' |t({'@key': current + 1}) }}><span class="visually-hidden">{{ 'Page' |t }} </span>{{ current + 1 }}</a>
          </li>
        {% endif %}
        {% if items.pages[current + 2] %}
          <li class="page-item">
            <a class="page-link" href="{{ items.pages[current + 2].href }}" title={{ 'Go to page @key' |t({'@key': current + 2}) }}><span class="visually-hidden">{{ 'Page' |t }} </span>{{ current + 2 }}</a>
          </li>
        {% endif %}
        {% if ellipses.next %}
          <li class="page-item" role="presentation">&hellip;</li>
        {% endif %}
        {% for item in items.pages %} {{ 1 }} {% endfor %}
        <li>{{ items.pages[17].href }}</li>
      {% endif %}

      {% if current > 1 and current < pager_max - 2 %}
        <li class="page-item">
          <a class="page-link" href="{{ items.pages[current - 1].href }}" title={{ 'Go to page @key' |t({'@key': current - 1}) }}><span class="visually-hidden">{{ 'Page' |t}}</span>{{ current - 1}}</a>
        </li>
        {% if items.pages[current] %}
          <li class="page-item active">
            <a class="page-link" href="{{ items.pages[current].href }}" title={{ 'Current page' |t}}><span class="visually-hidden">{{ 'Current Page' |t }} </span>{{ current }}</a>
          </li>
        {% endif %}
        {% if items.pages[current + 1] %}
          <li class="page-item">
            <a class="page-link" href="{{ items.pages[current + 1].href }}" title={{ 'Go to page @key' |t({'@key': current + 1}) }}><span class="visually-hidden">{{ 'Page' |t }} </span>{{ current + 1 }}</a>
          </li>
        {% endif %}
        {% if ellipses.next %}
          <li class="page-item" role="presentation">&hellip;</li>
        {% endif %}
      {% endif %}

      {% if current >= pager_max - 2 and current != pager_max %}{% endif %}
      {% if current == pager_max %}{% endif %}

      {# Render "Next page" button (either enabled or disabled) #}
      {% if items.next %}
        <li class="page-item">
      {% else %}
        <li class="page-item disabled">
      {% endif %}
          <a class="page-link page-link-icon" href="{{ items.next.href }}" title="{{ 'Go to next page'|t }}" rel="next"{{ items.next.attributes|without('href', 'title', 'rel') }}>
            <span class="visually-hidden">{{ 'Next page'|t }}</span>
            <span aria-hidden="true">{{ 'Next'|t }}</span>
          </a>
        </li>
    </ul>
    {# End pagination markup #}
  </nav>
{% endif %}
