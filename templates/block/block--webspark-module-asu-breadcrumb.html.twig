{#
/**
 * @file
 * Template for a banner block.
 */
#}
{% set classes = [
  'block',
  'block-' ~ configuration.provider|clean_class,
  'block-' ~ plugin_id|clean_class,
] %}

<div{{ attributes.addClass(classes) }}>
  {{ title_suffix }}
    {% include '@renovation/breadcrumb/breadcrumb.twig' with {
      links: content['#links'],
      bg_color: configuration['color'],
    } %}
</div>
