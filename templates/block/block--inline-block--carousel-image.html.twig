{#
/**
 * @file
 * Template for a banner block.
 */
#}
{% set classes = [
  'block',
  'block-' ~ configuration.provider|clean_class,
  'block-' ~ plugin_id|clean_class,
] %}

<div{{ attributes.addClass(classes) }}>
  {{ title_suffix }}
  {{ content.field_archon_menu_settings }}
  {{ content.field_tooltip }}
  {{ content|without('field_tooltip', 'field_archon_menu_settings') }}
</div>
